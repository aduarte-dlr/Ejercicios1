print(conteo)
print("\n")


n = 10
def fibonacci(n):
    secuencia = [0, 1]
    for i in range(2, n):
        secuencia.append(secuencia[i -1] + secuencia[i - 2])
        return secuencia
    
    resul_fibonacci = fibonacci(n)
    print(resul_fibonacci)
    print("\n")



    cadena = "open AI"
    resul = cadena.strip().replace("", "")
    print(resul)
    print("\n")



    inicio = 10
    fin = 30
    rango = inicio if inicio >= 10 and inicio <= 30 else None
    print(rango)
    print("\n")



    diccionario = ("uno": 1, "dos": 2)
    resultado = {v: k for k, v in diccionario.items()}
    print(resultado)
    print("\n")



    lista = []
    resul = len(lista)
    print(resul)
    print("\n")



    diccionario = {100: "uno", 200: "dos"}
    suma = sum(diccionario.keys())
    print(suma)
    print("\n")



    lista = ["examen", "python"]
mayusculas = [elemento.upper() for elemento in lista]
print(mayusculas)
